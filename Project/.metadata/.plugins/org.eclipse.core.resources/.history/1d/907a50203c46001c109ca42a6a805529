<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page import="java.sql.*" %>
<%@ page import="server.Member;" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

<%! String driverName = "com.mysql.jdbc.Driver"; %>
<%!String url = "jdbc:mysql://localhost:9999/userdb"; %>
<%!String user = "root"; %>
<%!String pass = "mysql"; %>
<%!String memberUser = member.getUserName(); %>
<form action="#">
<%
Connection con = null;
PreparedStatement ps = null;

try {
	Class.forName(driverName);
	con = DriverManager.getConnection(url, user, pass);
	String sql = "SELECT * FROM '" + memberUser + "'";
	ps = con.prepareStatement(sql);
	ResultSet rs = ps.executeQuery();
	%>
	<p>Select Name :
	<select>
	<%
	while (rs.next()) {
		String ftitle = rs.getString("title");
		%>
		<option value="<%=ftitle %>"><%=ftitle %></option>
		<%
	}	
	%>	
	</select>
	</p>
	<%
}
	catch(SQLException e)
	{
		out.println(e);
	}
	%>
</form>




</body>
</html>